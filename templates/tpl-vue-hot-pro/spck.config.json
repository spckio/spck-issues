{
  "run": {
    "type": "url",
    "tasks": {
      "if": [["!exists", "dist/bundle.js"]],
      "then": ["npm/install-dep", "npm/start"],
      "else": {
        "if": [["!urlExists", "http://localhost:9951"]],
        "then": "npm/start"
      }
    },
    "url": "http://localhost:9951",
    "ignoreCurrent": true
  }
}
